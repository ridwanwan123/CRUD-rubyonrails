<div id="<%= dom_id friend %>">

  <div class="form-group row">
    <label for="staticEmail" class="col-sm-2 col-form-label">First Name</label>
    <div class="col-sm-10">
    <label for="staticEmail" class="col-sm-2 col-form-label"><%= friend.first_name %></label>
    </div>
  </div>

  <div class="form-group row">
    <label for="staticEmail" class="col-sm-2 col-form-label">Last Name</label>
    <div class="col-sm-10">
      <label for="staticEmail" class="col-sm-2 col-form-label"><%= friend.last_name  %></label>
    </div>
  </div>

  <div class="form-group row">
    <label for="staticEmail" class="col-sm-2 col-form-label">Email</label>
    <div class="col-sm-10">
      <label for="staticEmail" class="col-sm-2 col-form-label"><%= friend.email  %></label>
    </div>
  </div>

  <div class="form-group row">
    <label for="staticEmail" class="col-sm-2 col-form-label">Phone</label>
    <div class="col-sm-10">
      <label for="staticEmail" class="col-sm-2 col-form-label"><%= friend.phone  %></label>
    </div>
  </div>

  <div class="form-group row">
    <label for="staticEmail" class="col-sm-2 col-form-label">Tiktok</label>
    <div class="col-sm-10">
      <label for="staticEmail" class="col-sm-2 col-form-label"><%= friend.tiktok  %></label>
    </div>
  </div>

</div>
